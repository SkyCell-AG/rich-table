{
  "name": "@skycell-ag/richtable",
  "version": "0.10.4",
  "private": false,
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkyCell-AG/RichTable.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint ./src",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "json-summary"
    ],
    "collectCoverageFrom": [
      "**/src/**",
      "!**/src/setupTests.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "__snapshots__",
      "setupTests.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@skycell-ag/theme": "^1.1.8",
    "array-flat-polyfill": "1.0.1",
    "lodash": "4.17.20",
    "prop-types": "15.7.2",
    "uuid": "8.3.2",
    "web-vitals": "0.2.4"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.0.4",
    "@mui/material": "^5.0.4",
    "@mui/styles": "^5.0.1",
    "clsx": "^1.1.1",
    "react": "17.0.2",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@emotion/styled": "11.3.0",
    "@storybook/addon-actions": "6.2.5",
    "@storybook/addon-essentials": "6.2.5",
    "@storybook/addon-knobs": "6.1.15",
    "@storybook/addon-links": "6.2.5",
    "@storybook/node-logger": "6.2.5",
    "@storybook/preset-create-react-app": "3.1.7",
    "@storybook/react": "6.2.5",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/react-hooks": "7.0.1",
    "@testing-library/user-event": "12.6.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.4.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.1",
    "eslint-config-airbnb": "18.2.1",
    "jest": "26.6.0",
    "jest-enzyme": "7.1.2",
    "jest-image-snapshot": "4.3.0",
    "jest-watch-typeahead": "0.6.5",
    "react-scripts": "4.0.3"
  },
  "description": "",
  "bugs": {
    "url": "https://github.com/SkyCell-AG/RichTable/issues"
  },
  "homepage": "https://github.com/SkyCell-AG/RichTable#readme",
  "author": "",
  "license": "MIT"
}
